{
  "name": "electron-svelte",
  "productName": "electron-svelte",
  "description": "Electron Svelte",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": ".vite/build/main.js",
  "author": {
    "name": "Pandoks_",
    "email": "35944715+Pandoks@users.noreply.github.com"
  },
  "scripts": {
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "start": "electron-forge start",
    "package": "vite build && electron-forge package",
    "make": "vite build && electron-forge make",
    "publish": "electron-forge publish"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.55.0",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.39.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^22.18.3",
    "electron": "^36.9.0",
    "jsdom": "^26.1.0",
    "svelte": "^5.38.10",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "electron",
      "electron-winstaller"
    ],
    "patchedDependencies": {
      "@sveltejs/kit": "patches/@sveltejs__kit.patch"
    }
  }
}
