{
	"extends": ["./tsconfig.json", "@electron-toolkit/tsconfig/tsconfig.web.json"],
	"compilerOptions": {
		"moduleResolution": "bundler",
		"composite": true,
		"noEmit": false,
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$app/types": [".svelte-kit/types/index.d.ts"],
			"@shared/*": ["shared/*"]
		},
		"outDir": "./.tsout/web"
	},
	"include": ["src/**/*", "electron/**/*", "shared/**/*"]
}
